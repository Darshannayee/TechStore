
@{
    ViewBag.Title = "Location";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<html>
<head>
    <style>
        #map {
            height: 400px;
            width: 400px;
        }

        html, body {
            height: 100%;
            margin: 0;
            padding: 0;
        }
    </style>
</head>
<body>
    <div id="map"></div>
    <script>
        var map;
        function initMap() {
            map = new google.maps.Map(document.getElementById("map"), {
                center: { lat: 43.653225, lng: -79.383186},
                zoom: 10
            
            });
        }
    </script>
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDqIOBtgwYgYiU_fa9vJ9CzO6E9MQyWh3Y&callback=initMap"
            async></script>
</body>
</html>